    <mj-style>
     @media screen and (max-width: 480px) {
      .dickies-logo {width: 200px !important;height: auto !important;}
      .men-section {display: none !important;padding: 0 !important;}
      .we-chose{width: 100% !important;}
      .check-our{padding: 0 0 0 10px !important; font-size: 18px !important;}
      .clothing-items{width: 148px !important;}
      .lower-dividers{padding:10px 0px !important;}
      .customer-service{font-size: 11px !important;}
      .preference{font-size: 11px !important;}
      .trademark{font-size: 11px !important;}
      }
    </mj-style>
    <mj-style>
      *{
        box-sizing: border-box;
      }
    </mj-style>
    <mj-style inline="inline">
      .header{
        background-color: #000;
        color: #ffffff;
      }
    </mj-style>
  </mj-head>
